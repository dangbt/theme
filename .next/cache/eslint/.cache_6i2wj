sce sit amet dui vitae urna tristique imperdiet. Donec eget sapien euismod, faucibus nibh non, consequat elit. Praesent sed vehicula.\"\n          />\n          <div className=\"col-span-1\" />\n        </div>\n      </div>\n    </div>\n  );\n}\n","/Users/dang/Documents/avenger/theme/src/components/home-page/index.tsx",[],[],"/Users/dang/Documents/avenger/theme/src/components/layout/footer.tsx",[],[],"/Users/dang/Documents/avenger/theme/src/components/layout/header.tsx",[],[],"/Users/dang/Documents/avenger/theme/src/components/layout/index.tsx",[],[],"/Users/dang/Documents/avenger/theme/src/components/layout/interface.ts",[],[],"/Users/dang/Documents/avenger/theme/src/components/pagination.tsx",["263","264","265","266"],[],"import { FILTER_DEFAULT } from 'contants/filter';\nimport React from 'react';\nimport { ChevronLeftIcon, ChevronRightIcon } from '@heroicons/react/solid';\nimport Button from 'components/button';\n\ninterface Props {\n  page: number;\n  totalPage: number;\n  handleChangePage: (page: number) => void;\n  handleChangeSize: (size: number) => void;\n  total: number;\n  size: number;\n}\n\nconst getPages = ({\n  page,\n  totalPage,\n}: {\n  page: Props['page'];\n  totalPage: Props['totalPage'];\n}): number[] => {\n  switch (page) {\n    case 1: {\n      if (totalPage === 1 || totalPage === 0) {\n        return [];\n      }\n      if (totalPage === 2) {\n        return [page, page + 1];\n      }\n      if (totalPage > 2) {\n        return [page, page + 1, page + 2];\n      } else {\n        return [page, page + 1];\n      }\n    }\n    case totalPage: {\n      if (totalPage === 1) {\n        return [];\n      }\n      if (totalPage === 2) {\n        return [1, 2];\n      }\n      return [totalPage - 2, totalPage - 1, totalPage];\n    }\n    default: {\n      if (totalPage === 1) {\n        return [];\n      }\n      if (totalPage === 2) {\n        return [page, page + 1];\n      }\n      return [page - 1, page, page + 1];\n    }\n  }\n};\n\nexport default function Pagination({\n  totalPage,\n  handleChangePage,\n  handleChangeSize,\n  page,\n  total,\n  size,\n}: Props) {\n  const handleChange = (p: Props['page']) => {\n    if (p === page) {\n      return;\n    }\n    handleChangePage(p);\n  };\n\n  const handleSelectSize = (e: React.ChangeEvent<HTMLSelectElement>) => {\n    handleChangeSize(Number(e.target.value));\n  };\n\n  const renderButton = () => {\n    return getPages({ page, totalPage }).map((p) => {\n      const actived = page === p;\n      if (actived) {\n        return (\n          <Button version=\"1\" key={p} onClick={() => handleChange(p)}>\n            <span className=\"text-white\">{p}</span>\n          </Button>\n        );\n      }\n      return (\n        <Button key={p} onClick={() => handleChange(p)}>\n          {p}\n        </Button>\n      );\n    });\n  };\n  return (\n    <div className=\"flex items-center justify-center space-x-[10px]\">\n      <Button disabled={page === 1} onClick={() => handleChangePage(1)}>\n        <ChevronLeftIcon width={20} />\n      </Button>\n      {renderButton()}\n      <Button\n        disabled={page === totalPage || totalPage === 0}\n        onClick={() => handleChangePage(totalPage)}\n      >\n        <ChevronRightIcon width={20} />\n      </Button>\n      {/* <Box>\n        <Select\n          border=\"1px\"\n          value={size || FILTER_DEFAULT.size}\n          onChange={handleSelectSize}\n        >\n          <option value=\"20\">20</option>\n          <option value=\"30\">30</option>\n          <option value=\"50\">50</option>\n          <option value=\"100\">100</option>\n        </Select>\n      </Box> */}\n    </div>\n  );\n}\n","/Users/dang/Documents/avenger/theme/src/contants/filter.ts",[],[],"/Users/dang/Documents/avenger/theme/src/fetcher/endpoint.ts",[],[],"/Users/dang/Documents/avenger/theme/src/fetcher/fetcher.ts",["267"],[],"import { get } from 'fetcher';\nimport { formatObjectToString } from 'utils/format-query';\nimport END_POINTS from './