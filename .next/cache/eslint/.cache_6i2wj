dleChangePage: (page: number) => void;\n  handleChangeSize: (size: number) => void;\n  total: number;\n  size: number;\n}\n\nconst getPages = ({\n  page,\n  totalPage,\n}: {\n  page: Props['page'];\n  totalPage: Props['totalPage'];\n}): number[] => {\n  switch (page) {\n    case 1: {\n      if (totalPage === 1 || totalPage === 0) {\n        return [];\n      }\n      if (totalPage === 2) {\n        return [page, page + 1];\n      }\n      if (totalPage > 2) {\n        return [page, page + 1, page + 2];\n      } else {\n        return [page, page + 1];\n      }\n    }\n    case totalPage: {\n      if (totalPage === 1) {\n        return [];\n      }\n      if (totalPage === 2) {\n        return [1, 2];\n      }\n      return [totalPage - 2, totalPage - 1, totalPage];\n    }\n    default: {\n      if (totalPage === 1) {\n        return [];\n      }\n      if (totalPage === 2) {\n        return [page, page + 1];\n      }\n      return [page - 1, page, page + 1];\n    }\n  }\n};\n\nexport default function Pagination({\n  totalPage,\n  handleChangePage,\n  handleChangeSize,\n  page,\n  total,\n  size,\n}: Props) {\n  const handleChange = (p: Props['page']) => {\n    if (p === page) {\n      return;\n    }\n    handleChangePage(p);\n  };\n\n  const handleSelectSize = (e: React.ChangeEvent<HTMLSelectElement>) => {\n    handleChangeSize(Number(e.target.value));\n  };\n\n  const renderButton = () => {\n    return getPages({ page, totalPage }).map((p) => {\n      const actived = page === p;\n      if (actived) {\n        return (\n          <Button version=\"1\" key={p} onClick={() => handleChange(p)}>\n            <span className=\"text-white\">{p}</span>\n          </Button>\n        );\n      }\n      return (\n        <Button key={p} onClick={() => handleChange(p)}>\n          {p}\n        </Button>\n      );\n    });\n  };\n  return (\n    <div className=\"flex items-center justify-center space-x-[10px]\">\n      <Button disabled={page === 1} onClick={() => handleChangePage(1)}>\n        <ChevronLeftIcon width={20} />\n      </Button>\n      {renderButton()}\n      <Button\n        disabled={page === totalPage || totalPage === 0}\n        onClick={() => handleChangePage(totalPage)}\n      >\n        <ChevronRightIcon width={20} />\n      </Button>\n      {/* <Box>\n        <Select\n          border=\"1px\"\n          value={size || FILTER_DEFAULT.size}\n          onChange={handleSelectSize}\n        >\n          <option value=\"20\">20</option>\n          <option value=\"30\">30</option>\n          <option value=\"50\">50</option>\n          <option value=\"100\">100</option>\n        </Select>\n      </Box> */}\n    </div>\n  );\n}\n","/Users/dang/Documents/avenger/theme/src/contants/filter.ts",[],[],"/Users/dang/Documents/avenger/theme/src/fetcher/endpoint.ts",[],[],"/Users/dang/Documents/avenger/theme/src/fetcher/fetcher.ts",["260"],[],"import { get } from 'fetcher';\nimport { formatObjectToString } from 'utils/format-query';\nimport END_POINTS from './endpoint';\n\nexport const getLayoutData = () => {\n  return get({ url: END_POINTS.LAYOUT });\n};\n\nexport const getTags = () => {\n  return get({ url: END_POINTS.TAG });\n};\n\nexport const getCategory = () => {\n  return get({ url: END_POINTS.CATEGORY });\n};\n\nexport const getBlogs = (objectQuery: any) => {\n  const url = `${END_POINTS.BLOG}?populate=*&&${formatObjectToString(\n    objectQuery,\n  )}`;\n  return get({ url });\n};\n\nexport const getBlogDetail = (slug: string) => {\n  const filters = {\n    filters: {\n      slug: {\n        $eq: slug,\n      },\n    },\n  };\n  const url = `${END_POINTS.BLOG}?populate=*&&${formatObjectToString(filters)}`;\n  return get({ url });\n};\n","/Users/dang/Documents/avenger/theme/src/fetcher/index.tsx",[],[],"/Users/dang/Documents/avenger/theme/src/i18n/en.ts",[],[],"/Users/dang/Documents/avenger/theme/src/i18n/index.ts",[],[],"/Users/dang/Documents/avenger/theme/src/i18n/vn.ts",[],[],"/Users/dang/Documents/avenger/theme/src/interface